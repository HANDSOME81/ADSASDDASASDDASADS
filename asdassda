local function VYUDYB_fake_script() -- ATATHubBETA.Message 
	local script = Instance.new('LocalScript', ATATHubBETA)

	local StarterGui = game:GetService("StarterGui")
	
	local Notifications = {
		"Notification Message 1",
	}
	
	local TimeBetweenNotifications = 5 --REPLACE 5 WITH THE NUMBER OF SECONDS YOU WANT TO WAIT IN BETWEEN NOTIFICATIONS APPEARING
	
	for i = 1, #Notifications do
		local Notification = Notifications[i]
			
		StarterGui:SetCore("SendNotification", {
			Title = "ATAT Hub Loaded!";
			Text = "Thank you for using ATAT Hub!";
			Duration = 7;
		})
			
		wait(TimeBetweenNotifications)
	end
end
coroutine.wrap(VYUDYB_fake_script)()
local function IHYD_fake_script() -- Menu.Script 
	local script = Instance.new('Script', Menu)

	local menustick = script.Parent.Frame
	local reanimstick = script.Parent.Parent.Reanim.Frame
	local hatstick = script.Parent.Parent.Hat.Frame
	local creditstick = script.Parent.Parent.Credits.Frame
	local Hubs = script.Parent.Parent.Parent.Hubs
	local Credits = Hubs.Credits
	local Menu = Hubs.Menu
	local Reanim = Hubs.Reanim
	local Hat = Hubs.Hat
	local button = script.Parent
	
	button.MouseButton1Click:Connect(function()
		menustick.Visible = true
		reanimstick.Visible = false
		hatstick.Visible = false
		creditstick.Visible = false
		Menu.Visible = true
		Credits.Visible = false
		Reanim.Visible = false
		Hat.Visible = false
	end)
end
coroutine.wrap(IHYD_fake_script)()
